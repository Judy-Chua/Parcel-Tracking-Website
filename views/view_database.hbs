<table id="popup">
    <tr>
        <th>ControlID</th>
        <th>Sender</th>
        <th>Receiver</th>
        <th>Item Description</th>
        <th>Status</th>
        <th>Actions</th>
    </tr>
    {{#each orders}}
        <tr>
            <td>{{this.orderId}}</td>
            <td>{{this.senderName}}</td>
            <td>{{this.receiverName}}</td>
            <td>{{this.itemDesc}}</td>
            <td>{{this.status}}</td>
            <td><a id="details" href="view_order={{this.orderId}}/more-details">Details</a></td>
        </tr>
    {{/each}}
</table>